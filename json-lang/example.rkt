#lang json-lang

["let", "int", "y", "=", 42,
      "in",
      ["let", "int", "x", "=", (10, "+", 11),
            "in",
            "y"]]

["let", "int", "x", "=", 42, "in",
      ["x", "<", 21]]

["let", "int", "x", "=", (10, "+", 5),
      "in",
      "x"]

[#t, "^", #f]

["let", "bool", "x", "=", #true,
      "in",
      [["let", "int", "x", "=", 42,
             "in",
             ["x", "<", 21]],
                            "||",
                            "x"]]
